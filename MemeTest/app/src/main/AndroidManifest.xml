<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jenns.memetest">

    <supports-screens
        android:largeScreens="true"
        android:normalScreens="true"
        android:smallScreens="true"
        android:xlargeScreens="false" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomePageActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".UserNameActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".ExpertMemeQuiz"
            android:screenOrientation="portrait"/>
        <activity android:name=".QuitalitySplashScreen"
            android:screenOrientation="portrait"/>
        <activity android:name=".BeginnerMemeQuiz"
            android:screenOrientation="portrait"/>
        <activity android:name=".IntermediateMemeQuiz"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>